{
  "name": "jsop",
  "version": "0.0.1",
  "description": "Javascript benchmark tool.",
  "main": "server.js",
  "engines": {
    "node": "~4"
  },
  "scripts": {
    "check": "node -c index.js && jscs index.js && node -c test/server.js && jscs test/server.js && echo 'OK!' || true",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "docker-redis": "docker run --rm -p 6379:6379 redis",
    "build-bundle": "cd build && node bundle.js"
  },
  "keywords": [
    "javascript",
    "benchmark",
    "tool"
  ],
  "author": "Harry Truong <harry@asdfjkltech.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:harrytruong/jsop.io.git"
  },
  "license": "MIT",
  "devDependencies": {
    "jscs": "^2.4.0",
    "mocha": "^2.3.3",
    "superagent-mock": "^1.8.0",
    "supertest": "^1.1.0",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "bootstrap-sass": "^3.3.5",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "connect-redis": "^3.0.1",
    "express": "^4.13.3",
    "express-session": "^1.12.0",
    "ioredis": "^1.10.0",
    "lodash": "^3.10.1",
    "superagent": "^1.4.0"
  }
}
