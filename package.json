{
  "name": "jsop",
  "version": "0.0.1",
  "description": "Javascript benchmark tool.",
  "main": "server.js",
  "engines": {
    "node": "~4"
  },
  "scripts": {
    "check": "node -c server.js && jscs server.js && node -c test/tests.js && jscs test/tests.js && echo 'OK!' || true",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/tests.js"
  },
  "keywords": [
    "javascript",
    "benchmark",
    "tool"
  ],
  "author": "Harry Truong <harry@asdfjkltech.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:harrytruong/jsop.io.git"
  },
  "license": "MIT",
  "devDependencies": {
    "jscs": "^2.4.0",
    "mocha": "^2.3.3",
    "redis-mock": "^0.5.0",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "connect-redis": "^3.0.1",
    "express": "^4.13.3",
    "express-session": "^1.12.0",
    "moment": "^2.10.6",
    "redis": "^2.2.5"
  }
}
